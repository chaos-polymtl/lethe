# Listing of Parameters
# ---------------------
# --------------------------------------------------
# Simulation and IO Control
#---------------------------------------------------
subsection simulation control
  set time step                  = 0.00001
  set step end       		 = 500000
  set n total              	 = 10
  set write frequency            = 100
end


#---------------------------------------------------
# Physical Properties
#---------------------------------------------------
subsection physical properties
    set gx            		 = 0.0
    set gy            		 = 0.0
    set gz            		 = -9.81
    set diameter            	 = 0.005
    set density            	 = 2500
    set Yp            	 	 = 50000000
    set Yw            	 	 = 50000000
    set vp            	 	 = 0.3
    set vw            	 	 = 0.3
    set ep           	 	 = 0.5
    set ew           	 	 = 0.5
    set mup         	         = 0.5
    set muw         	         = 0.5
    set murp         	         = 0.1
    set murw         	         = 0.1
end

#---------------------------------------------------
# Insertion Info
#---------------------------------------------------
subsection insertion info
    set Insertion time step           		 	= 4000
    set Inserted number of particles at each time step  = 10
    set Insertion frequency            		 	= 500
    set Insertion box minimum x            	 	= -0.05
    set Insertion box minimum y            	        = -0.05
    set Insertion box minimum z            	        = -0.05
    set Insertion box maximum x            	        = 0.05
    set Insertion box maximum y           	 	= 0.05
    set Insertion box maximum z            	        = 0.05
end

#---------------------------------------------------
# Properties of particles (for visualization)
#---------------------------------------------------
subsection output properties
    set Number of properties           		 	= 21
    set Number of fields 			        = 10
end
